{
    "dependencies": [
        "curl",
        "sdl2",
        {
            "name": "sdl2-image",
            "features": [
                "libjpeg-turbo",
                "libwebp"
            ]
        },
        "glew",
        {
            "name": "imgui",
            "features": [
                "sdl2-binding",
                "sdl2-renderer-binding",
                "opengl3-binding"
            ]
        },
        "gperf",
        "openssl",
        "gtest",
        "nlohmann-json",
        "utf8h",
        "tinyfiledialogs",
        "libssh",
        "zlib",
        "libpng",
        "sqlite3",
        {
            "name": "ffmpeg",
            "default-features": false,
            "features": [
                "avcodec",
                "avformat",
                "swresample"
            ]
        },
        "tinyxml2"
    ]
}