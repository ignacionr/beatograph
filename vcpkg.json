{
    "dependencies": [
        "curl",
        "sdl2",
        {
            "name": "sdl2-image",
            "features": [
                "libjpeg-turbo",
                "libwebp"
            ]
        },
        "glew",
        {
            "name": "imgui",
            "features": [
                "sdl2-binding",
                "sdl2-renderer-binding",
                "opengl3-binding"
            ]
        },
        "gtest",
        "nlohmann-json",
        "utf8h",
        "tinyfiledialogs",
        "cpr",
        "libssh",
        "librsvg",
        "zlib",
        "libpng",
        "sqlite3",
        {
            "name": "ffmpeg",
            "features": [
                "avcodec",
                "avformat"
            ]
        },
        "tinyxml2"
    ],
    "overrides": [
        {
            "name": "curl",
            "version": "7.80.0",
            "$reason": "https://github.com/microsoft/vcpkg/issues/23417"
        }
    ]
}